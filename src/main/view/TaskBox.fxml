<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>

<fx:root alignment="CENTER_LEFT" styleClass="taskBox" type="HBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="index" focusTraversable="false" text="Index" />
      <Label fx:id="recurringIcon" focusTraversable="false" text="Recurring" />
      <Label fx:id="description" focusTraversable="false" text="Description" />
      <Label fx:id="timeAndDate" focusTraversable="false" text="Time and Date" />
      <Label fx:id="countdown" text="Countdown" />
      <Label fx:id="priority" text="Priority" />
      <Label fx:id="group" text="Group" />
   </children>
</fx:root>
