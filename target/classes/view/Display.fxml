<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<fx:root alignment="TOP_CENTER" prefHeight="550.0" prefWidth="600.0" styleClass="display" type="VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <StackPane VBox.vgrow="ALWAYS">
         <children>
            <HBox alignment="TOP_CENTER">
               <children>
                  <VBox prefWidth="100.0">
                     <children>
                        <HBox fx:id="allFolder" prefHeight="100.0" prefWidth="200.0" />
                        <HBox fx:id="todayFolder" prefHeight="100.0" prefWidth="200.0" />
                        <HBox fx:id="nextSevenDayFolder" prefHeight="100.0" prefWidth="200.0" />
                        <ListView prefHeight="200.0" prefWidth="200.0" />
                     </children>
                  </VBox>
                  <ListView fx:id="taskView" prefWidth="500.0" />
               </children>
            </HBox>
            <VBox fx:id="noTaskOverlay" alignment="CENTER" prefHeight="450.0" prefWidth="450.0" StackPane.alignment="CENTER">
               <children>
                  <Label fx:id="noTaskOverlayIcon" text="Icon" />
                  <Label fx:id="noTaskOverlayGreeting" text="Greeting" />
                  <Label fx:id="noTaskOverlayMessage" text="Message" />
               </children>
            </VBox>
            <VBox fx:id="helpOverlay" alignment="CENTER" prefHeight="550.0" prefWidth="500.0" StackPane.alignment="CENTER">
               <children>
                  <Label fx:id="helpOverlayIcon" text="Help icon" />
                  <Label fx:id="helpOverlayTitle" text="Help title" />
                  <ListView fx:id="helpOverlayContents" prefHeight="200.0" prefWidth="200.0" />
               </children>
            </VBox>
         </children>
      </StackPane>
      <Label fx:id="feedbackLabel" text="Feedback" VBox.vgrow="ALWAYS" />
   </children>
</fx:root>
